<%- include("partials/header") %> 
<%- include("partials/nav",{isSignedIn,userId}) %> 
<%- include("partials/sub-nav", {title:"Welcome to YelpCamp", subTitle:"View all Camp Grounds from all over the world", data: "Click To Explore", url:"/campgrounds"}) %>

<% if(messages.error){%>
<div class="container alert alert-danger text-center">
  <%= messages.error %>
</div>
<%}%>
<% if(messages.success){%>
<div class="container alert alert-success text-center">
  <%= messages.success %>
</div>
<%}%>
<div class="container">
  <ul class="list-group">
    <% users.forEach(user => {%>
    <li class="list-group-item my-1 text-left">
      <a href="/<%= user._id %>/" class="btn btn-link">
        <% console.log(user) %> <%= user.username%>
        @
        <span class="text-muted"><%= user.email%></span>
        </a>
        <p class="text-secondary ml-3">Registered On: <%= moment(user.registeredOn).fromNow() %></p>
      
    </li>
    <%})%>
  </ul>
</div>

<%- include("partials/footer") %>
